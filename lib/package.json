{
  "name": "@browserbasehq/stagehand-lib",
  "version": "2.2.0",
  "private": true,
  "description": "Core Stagehand library sources",
  "main": "../dist/index.js",
  "module": "../dist/index.js",
  "types": "../dist/index.d.ts",
  "scripts": {
    "build-dom-scripts": "tsx dom/genDomScripts.ts",
    "build-js": "tsup index.ts --dts",
    "build-types": "tsc --emitDeclarationOnly --outDir ../dist",
    "build": "pnpm run build-dom-scripts && pnpm run build-js && pnpm run build-types",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@browserbasehq/sdk": "^2.5.0",
    "dotenv": "^16.5.0",
    "fs": "0.0.1-security",
    "os": "^0.1.2",
    "path": "^0.12.7",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "playwright-core": "^1.52.0",
    "zod-to-json-schema": "^3.24.5"
  },
  "peerDependencies": {
    "zod": "^3.24.3"
  },
  "optionalDependencies": {
    "@ai-sdk/anthropic": "^1.2.10",
    "@ai-sdk/azure": "^1.3.22",
    "@ai-sdk/cerebras": "^0.2.13",
    "@ai-sdk/deepseek": "^0.2.13",
    "@ai-sdk/google": "^1.2.14",
    "@ai-sdk/groq": "^1.2.8",
    "@ai-sdk/mistral": "^1.2.7",
    "@ai-sdk/openai": "^1.3.21",
    "@ai-sdk/perplexity": "^1.1.8",
    "@ai-sdk/togetherai": "^0.2.13",
    "@ai-sdk/xai": "^1.2.15",
    "@anthropic-ai/sdk": "^0.40.1",
    "@google/genai": "^0.12.0",
    "ai": "^4.3.13",
    "ollama-ai-provider": "^1.2.0",
    "openai": "^4.97.0"
  }
}